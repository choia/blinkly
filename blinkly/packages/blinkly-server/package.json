{
  "name": "blinkly-server",
  "version": "1.0.0",
  "main": "main.js",
  "type": "module",
  "author": "Alex Choi",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^18.11.18",
    "esbuild": "^0.17.5",
    "esbuild-register": "^3.4.2",
    "nodemon": "^2.0.20",
    "pino-pretty": "^9.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/swagger": "^8.3.0",
    "@fastify/swagger-ui": "^1.3.0",
    "@fastify/type-provider-typebox": "^2.4.0",
    "@prisma/client": "^4.9.0",
    "@sinclair/typebox": "^0.25.24",
    "@types/axios": "^0.14.0",
    "@types/jsonwebtoken": "^9.0.1",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.12.0",
    "jsonwebtoken": "^9.0.0",
    "prisma": "^4.9.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "esstart": "node -r esbuild-register src/main.ts",
    "esstart:dev": "yarn ts-node-dev -r esbuild-register --transpile-only src/main.ts | pino-pretty",
    "start": "ts-node-esm --transpileOnly ./src/main.ts | pino-pretty",
    "dev": "nodemon --watch ./src --exec \"ts-node-esm --transpileOnly ./src/main.ts | pino-pretty\" -e ts"
  }
}
